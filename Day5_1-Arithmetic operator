class ABC
{
	public static void main(String args[])
	{
	int a = 4;
	int b = 2;
	b+=a;
	int c = a++;
	int d = ++a;
	int e = --a;
	int f = a--;
	int g = a++ +b;
	System.out.println("values are" +a);
	System.out.println("values are" +b);
	System.out.println("values are" +c);
	System.out.println("values are" +d);
	System.out.println("values are" +e);
	System.out.println("values are" +f);
	System.out.println("values are" +g);
	}
}
