class relatiop {

 public static void main(String[] args) {
  
  int s1 = 21, s2 = 19;


  System.out.println("s1 > s2 = " + (s1 >  s2) );

  
  System.out.println("s1 <  s2 = " + (s1 <  s2) );

  
  System.out.println("s1 >= s2 = " + (s1 >= s2) );

  
  System.out.println("s1 <= s2 = " + (s1 <= s2) );

 }
}
