/*
  to check for even or odd number

  to find quotient we will use divider (/)
  to find reminder we will use modulus(%)

   */


  class EvenOdd  {
  	   public static void main(String args[])
  	   {
  	   	int a=90;
  	   	if(a%2==0)
  	   	{
  	   		System.out.println("a is even");
  	   	}
  	   	else {
  	   		System.out.println("a is odd");
  	   	}
  	   }
  }
